{
  "name": "voicednut",
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "^/[^.]+$",
      "dest": "/",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ]
}